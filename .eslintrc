{
  "parser": "babel-eslint",
    "rules": {
      "indent": [2, 2],
      "quotes": [2, "single"],
      "linebreak-style": [2, "unix"],
      "semi": [2, "always"],

      "comma-dangle": 0,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-empty": 2,
      "no-extra-semi": 2,
      "no-inner-declarations": 2,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-negated-in-lhs": 1,
      "no-sparse-arrays": 1,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 1,
      "no-unexpected-multiline": 1,

      // Best Practices
      "complexity": [1, 2],
      "curly": 2,
      "eqeqeq": 2,
      "no-alert": 1,
      "no-else-return": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 1,
      "no-implied-eval": 2,
      "no-iterator": 1,
      "no-lone-blocks": 2,
      "no-loop-func": 2,
      "no-multi-spaces": 1,
      "no-native-reassign": 1,
      "no-param-reassign": 1,
      "no-proto": 2,
      "no-redeclare": 1,
      "no-return-assign": 1,
      "no-self-compare": 2,
      "no-unused-expressions": 2,


      // Variables
      "no-delete-var": 1,
      "no-shadow": 1,
      "no-shadow-restricted-names": 1,
      "no-undef": 2,
      "no-undef-init": 1,
      "no-unused-vars": 2,

      // Stylistic Issues
      "array-bracket-spacing": [2, "never"],
      "camelcase": 1,
      "consistent-this": [2, "self"],
      "eol-last": 1,
      "key-spacing": 1,
      "max-nested-callbacks": 1,
      "new-cap": [2, {
        "capIsNew": false
      }],
      "new-parens": 1,
      "no-continue": 1,
      "no-lonely-if": 1,
      "no-multiple-empty-lines": 1,
      "no-trailing-spaces": 1,
      "sort-vars": 1,
      "space-before-blocks": 0,
      "space-before-function-paren": 0,
      "space-in-parens": 0,
      "space-infix-ops": 1,
      "space-return-throw-case": 1,
      "spaced-comment": 0,
      "wrap-regex": 0,

      // ECMAScript 6
      "constructor-super": 1,
      "generator-star-spacing": 0,
      "no-this-before-super": 1,
      "no-var": 1,
      "object-shorthand": 0,
      "prefer-const": 1
    },

    "env": {
      "browser": true,
      "node": false,
      "worker": false,
      "amd": false,
      "mocha": false,
      "jasmine": false,
      "phantomjs": false,
      "jquery": false,
      "prototypejs": false,
      "shelljs": false,
      "meteor": false,
      "mongo": false,
      "applescript": false,
      "es6": false
    },
    "plugins": [
      "react"
    ]
}
